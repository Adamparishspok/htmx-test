    "@storybook/test": "^7.6.10",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
